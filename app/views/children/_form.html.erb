<%= render "layouts/errors", :object => @child %>

<%= form_for @child do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :age %>
<%= f.number_field :age %>

<%= f.label :home %>
<%= f.text_field :home %>

<%= f.label :school %>
<%= f.text_field :school %>

<%= f.label :aspirations %>
<%= f.text_field :aspirations %>

<%= f.label :siblings %>
<%= f.number_field :siblings %>

<%= f.submit %>
<% end %>
